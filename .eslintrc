{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "standard",
    "eslint:recommended",
    "plugin:vue/essential"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "parser": "@typescript-eslint/parser",
    "requireConfigFile": false
  },
  "plugins": [
    "vue",
    "@typescript-eslint"
  ],
  "rules": {
    "comma-dangle": 0,
    "no-undef": 0,
    "no-unused-vars": 0,
    "vue/no-multiple-template-root": 0,
    "vue/multi-word-component-names": 0
  }
}
